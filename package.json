{
  "name": "nwb-electron-starter",
  "productName": "My Electron App",
  "version": "1.0.0",
  "description": "Describe nwb-electron-starter here",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "nwb build && babel main -d dist && npm run package",
    "watch-electron-main": "babel main -d dist -w",
    "clean": "nwb clean",
    "start": "babel main -d dist && node dev-server.js --run-electron",
    "test": "nwb test",
    "test:watch": "nwb test --server",
    "package": "rm -rf ./release && electron-packager --overwrite --asar='true' --ignore='src|main|tests' --prune --platform=darwin --arch=x64 --out='release' ."
  },
  "dependencies": {
    "electron-window-state": "^3.0.3",
    "flex-component": "^0.1.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "directory-named-webpack-plugin": "^1.1.0",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "^1.4.2",
    "express": "^4.14.0",
    "minimist": "^1.2.0",
    "nwb": "^0.12.2",
    "nwb-sass": "^0.6.0",
    "webpack": "^1.13.0"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
